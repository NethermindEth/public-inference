{
  "name": "public-inference",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "account": "yarn workspace @public-inference/foundry account",
    "account:generate": "yarn workspace @public-inference/foundry account:generate",
    "account:import": "yarn workspace @public-inference/foundry account:import",
    "chain": "yarn workspace @public-inference/foundry chain",
    "compile": "yarn workspace @public-inference/foundry compile",
    "deploy": "yarn workspace @public-inference/foundry deploy",
    "deploy:verify": "yarn workspace @public-inference/foundry deploy:verify",
    "flatten": "yarn workspace @public-inference/foundry flatten",
    "fork": "yarn workspace @public-inference/foundry fork",
    "format": "yarn next:format && yarn foundry:format",
    "foundry:format": "yarn workspace @public-inference/foundry format",
    "foundry:lint": "yarn workspace @public-inference/foundry lint",
    "foundry:test": "yarn workspace @public-inference/foundry test",
    "generate": "yarn account:generate",
    "postinstall": "husky install",
    "monitor:dev": "yarn workspace @public-inference/monitor dev",
    "monitor:build": "yarn workspace @public-inference/monitor build",
    "monitor:start": "yarn workspace @public-inference/monitor start",
    "monitor:lint": "yarn workspace @public-inference/monitor lint",
    "monitor:format": "yarn workspace @public-inference/monitor format",
    "next:build": "yarn workspace @public-inference/nextjs build",
    "next:check-types": "yarn workspace @public-inference/nextjs check-types",
    "next:format": "yarn workspace @public-inference/nextjs format",
    "next:lint": "yarn workspace @public-inference/nextjs lint",
    "next:serve": "yarn workspace @public-inference/nextjs serve",
    "precommit": "lint-staged",
    "start": "yarn workspace @public-inference/nextjs dev",
    "test": "yarn foundry:test",
    "vercel": "yarn workspace @public-inference/nextjs vercel",
    "vercel:yolo": "yarn workspace @public-inference/nextjs vercel:yolo",
    "verify": "yarn workspace @public-inference/foundry verify"
  },
  "devDependencies": {
    "husky": "~8.0.3",
    "lint-staged": "~13.2.2"
  },
  "packageManager": "yarn@3.2.3",
  "engines": {
    "node": ">=18.18.0"
  }
}
